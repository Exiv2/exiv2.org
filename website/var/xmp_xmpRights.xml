<?xml version = '1.0'?>
<?xml-stylesheet type="text/xsl" href="xmp.xsl"?>
<TAGLIST>
<HEADER>
<title>XMP tags defined in Exiv2</title>
<text>
<p>Some description</p>
<p>Click on a column header to sort the table.</p>
</text>
</HEADER>
<ROWSET>
   <ROW num="1">
      <tagname>Certificate</tagname>
      <title>Certificate</title>
      <xmpvaltype>URL</xmpvaltype>
      <type>XmpText</type>
      <category>External</category>
      <tagdesc>Online rights management certificate.</tagdesc>
   </ROW>
   <ROW num="2">
      <tagname>Marked</tagname>
      <title>Marked</title>
      <xmpvaltype>Boolean</xmpvaltype>
      <type>XmpText</type>
      <category>External</category>
      <tagdesc>Indicates that this is a rights-managed resource.</tagdesc>
   </ROW>
   <ROW num="3">
      <tagname>Owner</tagname>
      <title>Owner</title>
      <xmpvaltype>bag ProperName</xmpvaltype>
      <type>XmpBag</type>
      <category>External</category>
      <tagdesc>An unordered array specifying the legal owner(s) of a resource.</tagdesc>
   </ROW>
   <ROW num="4">
      <tagname>UsageTerms</tagname>
      <title>Usage Terms</title>
      <xmpvaltype>Lang Alt</xmpvaltype>
      <type>LangAlt</type>
      <category>External</category>
      <tagdesc>Text instructions on how a resource can be legally used.</tagdesc>
   </ROW>
   <ROW num="5">
      <tagname>WebStatement</tagname>
      <title>Web Statement</title>
      <xmpvaltype>URL</xmpvaltype>
      <type>XmpText</type>
      <category>External</category>
      <tagdesc>The location of a web page describing the owner and/or rights statement for this resource.</tagdesc>
   </ROW>
</ROWSET>
</TAGLIST>
